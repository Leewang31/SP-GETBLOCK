{
  "name": "getblock",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 4000",
    "build": "run-p type-check bulid-ssg",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "bulid-ssg": "vite-ssg build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format-test": "biome forma./src",
    "format": "biome format --write ./src"
  },
  "dependencies": {
    "lz-string": "1.5.0",
    "pinia": "2.1.4",
    "vue": "3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.2",
    "vue3-click-away": "1.2.4",
    "workbox-window": "^7.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.17",
    "@unhead/vue": "1.1.29",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/test-utils": "2.4.0",
    "@vue/tsconfig": "0.4.0",
    "jsdom": "22.1.0",
    "npm-run-all": "4.1.5",
    "sass": "1.63.6",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vite-plugin-pwa": "0.16.4",
    "vite-ssg": "0.23.0",
    "vitest": "0.32.4",
    "vue-tsc": "1.8.3"
  }
}
