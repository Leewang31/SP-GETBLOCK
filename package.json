{
  "name": "vue-typescript-boiler",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 4000",
    "build": "run-p type-check bulid-ssg",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "bulid-ssg": "vite-ssg build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "lz-string": "1.5.0",
    "pinia": "2.1.4",
    "storybook": "^7.0.25",
    "vue": "3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.2",
    "vue3-click-away": "1.2.4"
  },
  "devDependencies": {
    "@playwright/test": "1.35.1",
    "@rushstack/eslint-patch": "1.3.2",
    "@storybook/addon-essentials": "^7.0.25",
    "@storybook/addon-interactions": "^7.0.25",
    "@storybook/addon-links": "^7.0.25",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "^7.0.25",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.25",
    "@storybook/vue3-vite": "^7.0.25",
    "@tsconfig/node18": "18.2.0",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.17",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@unhead/vue": "1.1.29",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/test-utils": "2.4.0",
    "@vue/tsconfig": "0.4.0",
    "eslint": "8.44.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "9.15.1",
    "jsdom": "22.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "sass": "1.63.6",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vite-plugin-pwa": "0.16.4",
    "vite-ssg": "0.23.0",
    "vitest": "0.32.4",
    "vue-tsc": "1.8.3",
    "workbox-window": "7.0.0"
  }
}
